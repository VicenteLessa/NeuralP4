uracy)\n","test_accuracy_mean = sum(test_accuracy) / len(test_accuracy)\n","test_max = max(test_accuracy)\n","test_min = min(test_accuracy)\n","test_std = np.std(test_accuracy)\n","\n","print(f'accuracies_train: {train_accuracy}, accuracies_test: {test_accuracy}')\n","print(f'train_accuracy_mean: {train_accuracy_mean*100:.2f}, train_max: {train_max*100:.2f}, train_min: {train_min*100:.2f}, train_std: {train_std*100:.2f} \\n test_accuracy_mean: {test_accuracy_mean*100:.2f}, test_max: {test_max*100:.2f}, test_min: {test_min*100:.2f}, test_std: {test_std*100:.2f}')\n","\n"]},{"cell_type":"code","execution_count":57,"metadata":{"executionInfo":{"elapsed":4,"status":"ok","timestamp":1701091777942,"user":{"displayName":"Vicente Lessa","userId":"14551761473643883791"},"user_tz":180},"id":"B0QFHVCa5-Qz","colab":{"base_uri":"https://localhost:8080/","height":277},"outputId":"e5804ab7-25b7-47ad-93c2-b74a5bb4d15f"},"outputs":[{"output_type":"display_data","data":{"text/plain":["{'datetime': '2023-11-27 13:29:36.482073',\n"," 'training_dataset_shape': '(3550, 27)',\n"," 'test_dataset_shape': '(500, 27)',\n"," 'number_of_attrs': 27,\n"," 'n_epoch': 500,\n"," 'accuracy_train': 1.0,\n"," 'train_max': 1.0,\n"," 'train_min': 1.0,\n"," 'train_std': 0.0,\n"," 'accuracy_test': 0.9980000257492065,\n"," 'test_max': 0.9980000257492065,\n"," 'test_min': 0.9980000257492065,\n"," 'test_std': 0.0,\n"," 'accuracies_train': [1.0],\n"," 'accuracies_test': [0.9980000257492065]}"]},"metadata":{}}],"source":["# Date for report file\n","current_date_and_time = datetime.datetime.now()\n","current_date_and_time_string = str(current_date_and_time)\n","# Report file content\n","report = {\n","    \"datetime\": current_date_and_time_string,\n","    \"training_dataset_shape\":  str(df_train.shape),\n","    \"test_dataset_shape\": str(df_test.shape),\n","    \"number_of_attrs\": NUMBER_OF_ATTRS,\n","    \"n_epoch\":EPOCHS,\n","    \"accuracy_train\": train_accuracy_mean,\n","    \"train_max\": train_max,\n